<Window x:Class="Porównator_Baz.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Porównator_Baz"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TabControl>
            <TabItem Header="Usinięte jednostki">
                <TextBox x:Name="deletedUnit" VerticalScrollBarVisibility="Visible" />
            </TabItem>
                
            <TabItem Header="Dodane jednostki">
                <TextBox x:Name="addedUnit" VerticalScrollBarVisibility="Visible"/>
            </TabItem>
            
            <TabItem Header="Różnice - działki">
                <TextBox x:Name="differenceDzialki" VerticalScrollBarVisibility="Visible"/>
            </TabItem>
            <TabItem Header="Różnice - właściciele"></TabItem>
        </TabControl>
        
    </Grid>
</Window>
